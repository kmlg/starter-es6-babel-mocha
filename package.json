  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-es6": "^1.10.3",
    "eslint-plugin-commonjs": "^1.0.2",
    "eslint-plugin-mocha": "^6.3.0",
